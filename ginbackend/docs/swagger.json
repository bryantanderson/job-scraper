        "/scrape/seek": {
            "post": {
                "description": "Scrapes a seek job page for Software Engineering Jobs. Accepts a \"candidate\" and ranks the candidate against the job postings.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Scrape"
                ],
                "summary": "Scrapes a seek job page",
                "parameters": [
                    {
                        "description": "Request payload",
                        "name": "data",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/services.ScrapeSeekPayload"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/scrape/seek/{user}": {
            "get": {
                "description": "Fetches the assessments made for a particular candidate, in relation to scraped jobs.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Scrape"
                ],
                "summary": "Fetches the assessments made for a particular candidate, in relation to scraped jobs.",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User ID",
                        "name": "user",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
